{
  "name": "part-time-pay-backend",
  "version": "1.0.0",
  "description": "Backend API for Part-Time Pay$ job marketplace",
  "main": "dist/server.js",
  "scripts": {
    "dev": "nodemon src/server.ts",
    "dev:original": "nodemon src/server.ts",
    "build": "tsc",
    "build:simple": "tsc src/server-simple.ts --outDir dist",
    "start": "node dist/server.js",
    "start:original": "node dist/server.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --watchAll=false",
    "populate:blogs": "ts-node src/scripts/populateBlogs.ts",
    "populate:community": "ts-node src/scripts/populateCommunityPosts.ts",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "type-check": "tsc --noEmit"
  },
  "keywords": [
    "job",
    "marketplace",
    "api",
    "express",
    "typescript",
    "part-time"
  ],
  "author": "Part-Time Pay$ Team",
  "license": "MIT",
