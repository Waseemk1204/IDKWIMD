    "express": "^4.18.2",
    "express-mongo-sanitize": "^2.2.0",
    "express-rate-limit": "^7.1.5",
    "express-session": "^1.18.2",
    "express-validator": "^6.15.0",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
